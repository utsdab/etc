[
    {
        "keys": ["maya*"],
        "environment": {
            "MAYA_LOCATION": "$TR_ENV_MAYALOCATION",
            "LD_LIBRARY_PATH": "$TR_ENV_MAYALOCATION/lib:$LD_LIBRARY_PATH",
            "MAYA_MODULE_PATH": "$TR_ENV_MAYAVER/etc:$MAYA_MODULE_PATH",
            "PATH": "$MAYA_LOCATION/bin:$PATH"
        },
        "envhandler": "mayahandler"
    }
]
